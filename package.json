    "font-awesome": "^4.4.0",
    "github-markdown-css": "^2.2.1",
    "hatena-blog-api": "^0.3.0",
    "history": "^4.4.1",
    "markdown-it": "^8.1.0",
    "markdown-it-emoji": "^1.1.0",
    "markdown-it-highlightjs": "^2.0.0",
    "marked": "^0.3.5",
    "material-ui": "^0.16.4",
    "moment": "^2.11.2",
    "openurl": "^1.1.1",
    "react": "^15.4.1",
    "react-ace": "^4.1.0",
    "react-ace-wrapper": "^1.0.1",
    "react-dom": "^15.4.1",
